{
  "name": "webgl_engine",
  "version": "1.0.0",
  "description": "A little WebGL rendering engine. :D",
  "main": "dist/index.js",
  "scripts": {
    "build-prod": "tsc ; npm run bundle-index-prod ; npm run bundle-motion-prod ; npm run bundle-terrain-prod",
    "build-dev": "tsc ; npm run bundle-index-dev ; npm run bundle-motion-dev ; npm run bundle-terrain-dev",
    "build-index-prod": "tsc ; rollup --environment NODE_ENV:production,INPUT:dist/examples/index.js,OUTPUT:dist/builds/index.js -c",
    "build-index-dev": "tsc ; rollup --environment NODE_ENV:development,INPUT:dist/examples/index.js,OUTPUT:dist/builds/index.js -c",
    "build-motion-prod": "tsc ; rollup --environment NODE_ENV:production,INPUT:dist/examples/motion.js,OUTPUT:dist/builds/motion.js -c",
    "build-motion-dev": "tsc ; rollup --environment NODE_ENV:development,INPUT:dist/examples/motion.js,OUTPUT:dist/builds/motion.js -c",
    "build-terrain-prod": "tsc ; rollup --environment NODE_ENV:production,INPUT:dist/examples/terrain.js,OUTPUT:dist/builds/terrain.js -c",
    "build-terrain-dev": "tsc ; rollup --environment NODE_ENV:development,INPUT:dist/examples/terrain.js,OUTPUT:dist/builds/terrain.js -c",
    "bundle-index-prod": "rollup --environment NODE_ENV:production,INPUT:dist/examples/index.js,OUTPUT:dist/builds/index.js -c",
    "bundle-index-dev": "rollup --environment NODE_ENV:development,INPUT:dist/examples/index.js,OUTPUT:dist/builds/index.js -c",
    "bundle-motion-prod": "rollup --environment NODE_ENV:production,INPUT:dist/examples/motion.js,OUTPUT:dist/builds/motion.js -c",
    "bundle-motion-dev": "rollup --environment NODE_ENV:development,INPUT:dist/examples/motion.js,OUTPUT:dist/builds/motion.js -c",
    "bundle-terrain-prod": "rollup --environment NODE_ENV:production,INPUT:dist/examples/terrain.js,OUTPUT:dist/builds/terrain.js -c",
    "bundle-terrain-dev": "rollup --environment NODE_ENV:development,INPUT:dist/examples/terrain.js,OUTPUT:dist/builds/terrain.js -c"
  },
  "keywords": [
    "WebGL",
    "OpenGL",
    "Rendering",
    "Engine",
    "3D",
    "GPU"
  ],
  "author": "Hirevo",
  "license": "ISC",
  "dependencies": {
    "rollup": "^0.50.0",
    "rollup-plugin-uglify": "^2.0.1",
    "typescript": "^2.5.3",
    "uglify-es": "^3.1.5"
  },
  "devDependencies": {}
}
